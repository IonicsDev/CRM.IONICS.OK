@model List<CRM.Business.Entities.NovedadUsuarios>

@if (Model != null)
{
    <ul>
        @for (int i = 0; i < Model.Count; i++)
        {
            <li>
                @Html.Label(Model[i].Usuario.UserName)
                <input type="hidden" name="Usuarios" value="@Model[i].Usuario_Id" />
                <input type="hidden" name="UsuariosNotificar[@i]" value="@Model[i].Usuario_Id" />
            </li>
        }
    </ul>
}


